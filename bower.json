{
  "name": "laima",
  "version": "0.1.0",
  "dependencies": {
    "jquery": "1.11.2",
    "backbone": "1.1.2",
    "requirejs": "2.1.17",
    "requirejs-text": "2.0.14",
    "requirejs-i18n": "2.0.4"
  },
  "ignore": [
    "test"
  ],
  "devDependencies": {
    "mockjs": "~0.1.8",
    "mocha": "~2.2.1",
    "chai": "~2.2.0"
  },
  "resolutions": {
    "jquery": "1.11.2"
  },
  "overrides": {
    "jquery": {
      "ignore": [
        "*.json",
        "*.txt",
        "src/**"
      ]
    },
    "backbone": {
      "ignore": [
        "*.md",
        "*.html",
        "CNAME",
        "LICENSE",
        ".*",
        "*.json",
        "index.js"
      ]
    },
    "requirejs": {
      "ignore": [
        "*.json",
        "*.md"
      ]
    },
    "requirejs-i18n": {
      "ignore": [
        "*.json",
        "*.md",
        "LICENSE"
      ]
    },
    "requirejs-text": {
      "ignore": [
        "*.json",
        "*.md",
        "LICENSE"
      ]
    },
    "underscore": {
      "ignore": [
        "*.json",
        "*.md",
        "LICENSE"
      ]
    }
  }
}
